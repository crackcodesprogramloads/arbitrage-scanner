datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Watchlist {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  authId   String   @unique
  coinList String[] @db.ObjectId
}

model Coin {
  id     String @id @default(auto()) @map("_id") @db.ObjectId
  name   String
  ticker String
}
